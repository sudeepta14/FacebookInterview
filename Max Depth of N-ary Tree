class Solution {
    public int maxDepth(Node root) {
         List<Integer> heights = new LinkedList<>();
        if(root == null){
            return 0;
        }else if(root.children.isEmpty()){
            return 1;
        }else{
            
           
            for(Node item: root.children){
                heights.add(maxDepth(item));
            }
            
        }
        return Collections.max(heights) + 1;
    }
}

DFS
